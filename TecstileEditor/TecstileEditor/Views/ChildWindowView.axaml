<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="using:TecstileEditor.ViewModels"
        xmlns:view="using:TecstileEditor.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="TecstileEditor.Views.ChildWindowView"
        x:DataType="vm:ChildWindowViewModel"
        Title="ChildWindow">
  <StackPanel>
    <Button Content="Welcome"></Button>
    <Button>to</Button>
    <Button Content="{Binding CurrentEditorName}"></Button>
    <ContentControl Content = "{Binding CurrentEditor}"/>
  </StackPanel>
</Window>
