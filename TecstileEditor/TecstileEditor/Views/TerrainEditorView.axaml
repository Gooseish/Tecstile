<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:vm="clr-namespace:TecstileEditor.ViewModels"
             x:DataType="vm:TerrainEditorViewModel"
             x:Class="TecstileEditor.Views.TerrainEditorView">
  <StackPanel Orientation="Horizontal">
    <!-- Select terrain from dictionary -->
      <StackPanel>
        <TextBlock Text="All Terrain"/>
        <ScrollViewer>
          <ItemsControl ItemsSource="{Binding TerrainData}">
            
          </ItemsControl>
        </ScrollViewer>
        <Button Command="{Binding AddTerrainDataCommand}">New Terrain</Button>
      </StackPanel>
  </StackPanel>
</UserControl>
